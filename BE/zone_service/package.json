{
  "name": "zone_service",
  "version": "1.0.0",
  "description": "Zone Service - Microservice for zone and location management",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:prod": "node dist/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "tsx index.ts",
    "seed:zones": "tsx processors/zones-seeder.ts",
    "seed:roads": "tsx processors/roads-seeder.ts",
    "seed:addresses": "tsx processors/overpass-addresses-seeder.ts",
    "seed:addresses-old": "tsx processors/addresses-seeder.ts",
    "extract:thuduc": "tsx processors/extract-thuduc-roads.ts",
    "osrm:generate": "tsx processors/generate-osrm-data.ts",
    "osrm:build": "node scripts/osrm-manager.js build",
    "osrm:build:1": "node scripts/osrm-manager.js build 1",
    "osrm:build:2": "node scripts/osrm-manager.js build 2",
    "osrm:start:1": "node scripts/osrm-manager.js start 1",
    "osrm:start:2": "node scripts/osrm-manager.js start 2",
    "osrm:stop:1": "node scripts/osrm-manager.js stop 1",
    "osrm:stop:2": "node scripts/osrm-manager.js stop 2",
    "osrm:restart": "node scripts/osrm-manager.js restart",
    "osrm:status": "node scripts/osrm-manager.js status",
    "osrm:health": "node scripts/osrm-manager.js health",
    "check:connectivity": "tsx processors/check-road-connectivity.ts",
    "test:routing": "tsx test/test-osrm-hard-routes.ts",
    "test:stress": "tsx test/test-service-stress.ts",
    "test:traffic": "tsx test/test-traffic-service.ts",
    "test:all": "tsx test/test-all-comprehensive.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run prisma:generate"
  },
  "keywords": [
    "microservice",
    "zone",
    "location",
    "geospatial"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.5",
    "prisma": "^6.16.3",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@turf/turf": "^7.1.0",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "kafkajs": "^2.2.4",
    "ngeohash": "^0.6.3",
    "osm-pbf-parser": "^2.3.0",
    "pbf": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.18.3"
  },
  "prisma": {
    "schema": "./prisma"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
